  "description": "JSP 转 React 的 AI Agent CLI 工具 - 渐进式迁移解决方案",
  "main": "src/cli.js",
  "bin": {
    "jsp2react": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js",
    "convert": "node src/cli.js convert",
    "analyze": "node src/cli.js analyze",
